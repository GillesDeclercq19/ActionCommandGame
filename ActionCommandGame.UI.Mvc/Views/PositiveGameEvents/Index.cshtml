@model IList<PositiveGameEventResult>
@{
    ViewData["Title"] = "PositiveGameEvents";
}

<h1>@ViewData["Title"]</h1>

<a class="btn btn-primary" asp-action="Create">New PositiveGameEvent</a>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Zeni</th>
            <th>Experience</th>
            <th>Probability</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var positiveGameEvent in Model)
        {
            <partial name="RowPartial" model="positiveGameEvent" />
        }
    </tbody>
</table>
